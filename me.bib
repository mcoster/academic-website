@Article{Schwartz2015a,
  author        = {Schwartz, Brett D. and Skinner-Adams, Tina S. and Andrews, Katherine T. and Coster, Mark J. and Edstein, Michael D. and MacKenzie, Donna and Charman, Susan A. and Koltun, Maria and Blundell, Scott and Campbell, Anna and Pouwer, Rebecca H. and Quinn, Ronald J. and Beattie, Karren D. and Healy, Peter C. and Davis, Rohan A.},
  title         = {Synthesis and antimalarial evaluation of amide and urea derivatives based on the thiaplakortone A natural product scaffold.},
