@Article{Dong2011,
  author        = {Dong, Lan-Feng and Jameson, Victoria J. A. and Tilly, David and Prochazka, Lubomir and Rohlena, Jakub and Valis, Karel and Truksa, Jaroslav and Zobalova, Renata and Mahdavian, Elahe and Kluckova, Katarina and Stantic, Marina and Stursa, Jan and Freeman, Ruth and Witting, Paul K. and Norberg, Erik and Goodwin, Jacob and Salvatore, Brian A. and Novotna, Jana and Turanek, Jaroslav and Ledvina, Miroslav and Hozak, Pavel and Zhivotovsky, Boris and Coster, Mark J. and Ralph, Stephen J. and Smith, Robin A. J. and Neuzil, Jiri.},
  title         = {Mitochondrial targeting of α-tocopheryl succinate enhances its pro-apoptotic efficacy: A new paradigm for effective cancer therapy.},
  journal       = {Free Radical Biology \& Medicine},
  year          = {2011},
  volume        = {50},
  number        = {11},
  pages         = {1546--1555},
  issn          = {0891-5849},
  __markedentry = {[s2598056:6]},
  abstract      = {Mitochondria are emerging as intriguing targets for anti-cancer agents.  We tested here a novel approach, whereby the mitochondrially targeted delivery of anti-cancer drugs is enhanced by the addn. of a triphenylphosphonium group (TPP+).  A mitochondrially targeted analog of vitamin E succinate (MitoVES), modified by tagging the parental compd. with TPP+, induced considerably more robust apoptosis in cancer cells with a 1-2 log gain in anti-cancer activity compared to the unmodified counterpart, while maintaining selectivity for malignant cells.  This is because MitoVES assocs. with mitochondria and causes fast generation of reactive oxygen species that then trigger mitochondria-dependent apoptosis, involving transcriptional modulation of the Bcl-2 family proteins.  MitoVES proved superior in suppression of exptl. tumors compared to the untargeted analog.  We propose that mitochondrially targeted delivery of anti-cancer agents offers a new paradigm for increasing the efficacy of compds. with anti-cancer activity. [on SciFinder(R)]},
  comment       = {CAPLUS AN 2011:581265(Journal)},
  keywords      = {cytotoxic antioxidant vitamin E succinate triphenylphosphonium apoptosis cancer fibroblast},
  publisher     = {Elsevier B.V.},
}

